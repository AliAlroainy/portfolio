
<!-- Modal -->
<form method="post" action="/add_info" class="modal fade" id="personalModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Edit info</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        
        <div class="input-group mb-3">
          <span class="input-group-text" id="basic-addon1">name</span>
          <input type="text" class="form-control"  aria-label="name"  name="pname" aria-describedby="basic-addon1">
        </div>

        <div class="input-group mb-3">
          <span class="input-group-text" id="basic-addon1">job</span>
          <input type="text" class="form-control"  aria-label="job" name="job" aria-describedby="basic-addon1">
        </div>
        <!-- <label for="basic-url" class="form-label">photo</label>
        <div class="input-group mb-3">
          <input type="file" class="form-control" id="basic-url" name="img" aria-describedby="basic-addon3">
        </div> -->

        <div class="input-group mb-3">
          <span class="input-group-text" id="basic-addon1">sentence</span>
          <input type="text" class="form-control"  aria-label="sentence" name="sentence" aria-describedby="basic-addon1">
        </div>
  
        
 
        
        <div class="input-group">
          <span class="input-group-text">description</span>
          <textarea class="form-control" aria-label="With textarea" name="description" ></textarea>
        </div>


      </div>
      <div class="modal-footer">
        <button type="reset" class="btn btn-secondary" style="background-color: transparent; border: 1px solid #ffeba7;" data-bs-dismiss="modal">Cancel</button>
        <button type="submit" class="btn btn-primary" >submit </button>
      </div>
    </div>
  </div>
</form>

<!--end personal model-->






