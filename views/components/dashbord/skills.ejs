
                
                <div class="row gutters-sm mt-0" id="sk">
                    <div class="col-md-12 mb-3">
                      <div class="card h-100">
                        <div class="card-body">
  
                          <h6  class="d-flex align-items-center mb-3"><i class="material-icons text-info mr-2">skills</i></h6>
                         <% for (const item in skills) {
                           if (Object.hasOwnProperty.call(skills, item)) {
                             const element = skills[item]; %>
                          
                          <div id="ittem" class="mb-3">
                            <small ><%= element.sname; %> </small>

                            <div id="sinpt" style="visibility: hidden ;">
                            <input type="text"  value="<%= element.sname; %>"  />
                            <button onclick="hid();">save</button>
                          </div>
                            <div class="d-flex flex-row  mt-2 w-25" style="direction: rtl;border-top: solid 1px rgba(141, 140, 140, 0.609);">
                              <div id="dlt"   style="width: 20px; height: 20px; cursor: pointer;"> <i class="bi bi-check-circle"></i> </div>
                              <form id="dlt" action="/dashboard/<%=element.id%> ?_method=DELETE " method="post"   style="width: 20px; height: 20px; cursor: pointer;"> <button style="border: none; background-color: transparent;"><i class="bi bi-trash"></i></button>  </form>
                              <div id="#"   style="width: 20px; height: 20px; cursor: pointer;" onclick="sedt();" >  <i class="bi bi-pencil-square"></i> </div>
                              <script>
                                var x = document.getElementById('sinpt');
                                function sedt(){
                                  x.style.visibility = "visible";
                                }

                                function hid(){
                                  x.style.visibility = "hidden";
                                }
                                </script>
                            </div> 
  
                          </div>

                          <%   
                           }
                         }
                         %>

                       <div class="row">
                            <div class="col-sm-12">
                              <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#skillsModal" >
                                <i class="bi bi-plus-circle"></i>
                                add
                               </button>
  
                          </div>
                          </div>
                        </div>
                      </div>
                    </div>
                    </div>
  
  